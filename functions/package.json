    "firebase-functions": "^6.0.1"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "firebase-functions-test": "^3.1.0",
    "prettier": "^3.5.3"
  },
  "private": true,
  "prettier": {
    "trailingComma": "all"
  }
